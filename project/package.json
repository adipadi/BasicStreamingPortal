    "expect": "1.20.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-cli": "1.2.2",
    "minami": "1.1.1",
    "node-sass": "3.10.1",
    "postcss-loader": "0.13.0",
    "query-string": "4.2.3",
