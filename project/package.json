{
  "name": "vimond-cms",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "start": "node src/server/server.js",
    "start-dev": "better-npm-run start:dev",
    "start-backend": "better-npm-run start:backend",
    "start-dev-db": "better-npm-run start:devDb",
    "start-dev-no-jwt": "better-npm-run start:dev-no-jwt",
    "migrate-start": "NODE_ENV=migrate node migration/migrate.js && NODE_ENV=production node src/server/server.js",
    "test": "better-npm-run test:all",
    "test-server": "better-npm-run test:server",
    "test-frontend": "better-npm-run test:frontend",
    "test:watch": "npm test -- --watch",
    "deploy": "better-npm-run build:prod",
    "dev": "better-npm-run build:dev",
    "migrate": "better-npm-run migrate",
    "migrate-dev": "better-npm-run migrate-dev",
    "deploy:imageselector": "better-npm-run build:imageselector",
    "deploy:imagecropper": "better-npm-run build:imagecropper",
    "lint": "eslint lib/** src/client/** routes/** --ext .js --ext .jsx || true",
    "lint-backend": "./node_modules/.bin/eslint src/server/ lib",
    "outdated": "node ./scripts/pkg-outdated.js minor=5",
    "revision": "sh scripts/update_revision.sh > revision.json",
    "docs": "jsdoc -c jsdoc.json ./src/client/**",
    "generate-docs-and-deploy": "jsdoc -c jsdoc.json ./src/client/** && ssh ubuntu@files.vimond.com 'mkdir -p /var/www/default/htdocs/files/docs/vimond-cms' && scp -r ./dist/docs/* ubuntu@files.vimond.com:/var/www/default/htdocs/files/docs/vimond-cms"
  },
  "betterScripts": {
    "start:dev": {
      "command": "echo \"\n\n***\nDEPRECATED! USE npm run start-backend INSTEAD\n***\n\n\" "
    },
    "start:backend": {
      "command": "node src/server/server.js",
      "env": {
        "NODE_ENV": "development",
        "CMS_PORT": "3080",
        "CMS_API_SECRET": "{SSHA}7cFtZruWJq7itGdtgI28K7/wRm07qybrm/T28w==",
        "CMS_SMTP_PASSWORD": "1f2unf1fglq6",
        "CMS_TENANTS": "[{'value': 'vimond', 'label': 'Vimond' },{ 'value': 'comcast', 'label': 'Comcast' },{ 'value': 'comcastuniversity', 'label': 'Comcast University'}]",
        "CMS_FEATURE_ADMIN_MODULE": "on",
        "CMS_FEATURE_LICENSE_MODULE": "on",
        "CMS_VIMOND_CMS_URL_EXTERNAL": "http://localhost:3080/",
        "VCC_IMAGE_BANK_SELECTOR_FEATURE_TOGGLE": "true",
        "FEATURE_TOGGLE_IMAGE_CROPPER": "true",
        "FEATURE_TOGGLE_IMAGE_SERVICE_BEARER_TOKEN": "true",
        "CMS_JWT_SECRET": "thisisasecret"
      }
    },
    "start:dev-no-jwt": {
      "command": "npm run revision && nodemon src/server/server.js",
      "env": {
        "NODE_ENV": "development",
        "CMS_PORT": "3080",
        "CMS_API_SECRET": "{SSHA}7cFtZruWJq7itGdtgI28K7/wRm07qybrm/T28w==",
        "CMS_SMTP_PASSWORD": "1f2unf1fglq6",
        "CMS_TENANTS": "[{'value': 'vimond', 'label': 'Vimond' },{ 'value': 'comcast', 'label': 'Comcast' },{ 'value': 'comcastuniversity', 'label': 'Comcast University'}]",
        "CMS_FEATURE_ADMIN_MODULE": "on",
        "CMS_FEATURE_LICENSE_MODULE": "on",
        "VCC_IMAGE_BANK_SELECTOR_FEATURE_TOGGLE": "true",
        "FEATURE_TOGGLE_IMAGE_CROPPER": "true"
      }
    },
    "start:devDb": {
      "command": "npm run revision && node src/server/server.js",
      "env": {
        "NODE_ENV": "development",
        "CMS_PORT": "3080",
        "CMS_API_SECRET": "{SSHA}7cFtZruWJq7itGdtgI28K7/wRm07qybrm/T28w==",
        "CMS_SMTP_PASSWORD": "1f2unf1fglq6",
        "CMS_TENANTS": "[{'value': 'vimond', 'label': 'Vimond' },{ 'value': 'comcast', 'label': 'Comcast' },{ 'value': 'comcastuniversity', 'label': 'Comcast University'}]",
        "CMS_FEATURE_ADMIN_MODULE": "on",
        "CMS_FEATURE_LICENSE_MODULE": "on",
        "CMS_LICENSE_SERVICE_URL": "http://localhost:3081/",
        "CMS_LICENSE_SERVICE_URL_EXTERNAL": "http://localhost:3081/",
        "CMS_VIMOND_CMS_URL_EXTERNAL": "http://localhost:3080/",
        "CMS_DB_USER": "postgres",
        "CMS_DB_PASSWORD": "",
        "CMS_DB_HOST": "localhost",
        "CMS_DB_DBNAME": "massive",
        "CMS_JWT_SECRET": "thisisasecret"
      }
    },
    "build:prod": {
      "command": "gulp cleanAndCopy && webpack -p --progress --config webpack.prod.config.js && gulp copyPublic && gulp copyDist && gulp validateBuild",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "gulp cleanAndCopy && webpack-dev-server --config webpack.dev.config.js -d --progress --colors --inline --hot --content-base public",
      "env": {
        "NODE_ENV": "development",
        "CMS_LICENSE_SERVICE_URL": "http://localhost:3081/"
      }
    },
    "migrate": {
      "command": "node migration/migrate.js",
      "env": {
        "NODE_ENV": "migrate"
      }
    },
    "migrate-dev": {
      "command": "node migration/migrate.js",
      "env": {
        "NODE_ENV": "migrate",
        "CMS_DB_USER": "postgres",
        "CMS_DB_PASSWORD": "",
        "CMS_DB_HOST": "localhost",
        "CMS_DB_DBNAME": "massive"
      }
    },
    "build:imageselector": {
      "command": "gulp cleanAndCopy && webpack -p --progress --config webpack.imageselector.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:imagecropper": {
      "command": "gulp cleanAndCopy && webpack -p --progress --config webpack.imagecropper.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:frontend": {
      "command": "webpack --config webpack.test.config.js && ./node_modules/mocha/bin/mocha -r mock-local-storage -r ./test-setup ./tmp/testbundle.js --compilers js:babel-core/register --recursive"
    },
    "test:server": {
      "command": "./node_modules/mocha/bin/mocha test/server/**/*-test.js",
      "env": {
        "NODE_ENV": "test",
        "FEATURE_TOGGLE_USE_LOCAL_NAVIGATION": "true"
      }
    },
    "test:all": {
      "command": "./node_modules/mocha/bin/mocha test/**/*-test.js",
      "env": {
        "NODE_ENV": "test",
        "FEATURE_TOGGLE_USE_LOCAL_NAVIGATION": "true"
      }
    }
  },
  "dependencies": {
    "assert": "1.4.1",
    "async": "2.0.1",
    "autoprefixer": "6.5.0",
    "aws-sdk": "^2.4.0",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-polyfill": "6.16.0",
    "babelify": "7.3.0",
    "body-parser": "1.15.2",
    "chalk": "*",
    "classnames": "2.2.5",
