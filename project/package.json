  "private": true,
  "scripts": {
    "start": "node src/server/server.js",
    "dev": "better-npm-run build:dev",
    "start-backend": "better-npm-run start:backend"
  },
  "betterScripts": {
    "start:backend": {
      "command": "node src/server/server.js",
      "env": {
        "NODE_ENV": "development",
        "CMS_PORT": "3080"
      }
    },
    "build:dev": {
      "command": "gulp cleanAndCopy && webpack-dev-server --config webpack.dev.config.js -d --progress --colors --inline --hot --content-base public",
